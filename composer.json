    "suggest": {
        "illuminate/support": "Required for Laravel Service Provider and Facade support (^8.0|^9.0|^10.0|^11.0)"
    },
    "extra": {
        "laravel": {
            "providers": [
                "CHYP\\Partner\\Echooss\\Voucher\\Laravel\\EchossVoucherServiceProvider"
            ],
            "aliases": {
                "EchossVoucher": "CHYP\\Partner\\Echooss\\Voucher\\Laravel\\Facades\\EchossVoucher"
