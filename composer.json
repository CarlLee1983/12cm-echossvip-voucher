{
    "name": "chyp-partner-api-sdk/echoss-voucher",
    "description": "12CM echoss VIP API package",
    "type": "library",
    "license": "MIT",
    "version": "1.3.0",
    "authors": [
        {
            "name": "Carl",
            "email": "cylee@chyp.com.tw"
        }
    ],
    "minimum-stability": "stable",
    "autoload": {
